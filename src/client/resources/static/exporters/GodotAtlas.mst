{
	"textures": [
		{
			"image": "{{config.imageFile}}",
			"size": {
				"w": {{config.imageWidth}},
				"h": {{config.imageHeight}}
			},
			"sprites": [
				{{#rects}}
				{
					"filename": "{{{name}}}",
					"region": {
						"x": {{frame.x}},
						"y": {{frame.y}},
						"w": {{frame.w}},
						"h": {{frame.h}}
					},
					"margin": {
						"x": 0,
						"y": 0,
						"w": {{32 - frame.w}},
						"h": {{32 - frame.h}}
					}
				}{{^last}},{{/last}}
				{{/rects}}
			]
		}
	],
	"meta": {
		"app": "{{{appInfo.url}}}",
		"version": "{{appInfo.version}}",
		"format": "{{config.format}}",
	}
}